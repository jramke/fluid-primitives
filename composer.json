{
    "name": "jramke/fluid-primitives",
    "type": "typo3-cms-extension",
    "description": "The headless component library for TYPO3 Fluid",
    "license": "GPL-2.0-or-later",
    "version": "0.2.1",
    "authors": [
        {
            "name": "Joost Ramke",
            "role": "Developer",
            "email": "hey@joostramke.com",
            "homepage": "https://joostramke.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "typo3/cms-fluid": "^13.4",
        "typo3fluid/fluid": "^4"
    },
    "scripts": {
        "prepare-release": ["rm -rf .github", "rm .gitignore"]
    },
    "autoload": {
        "psr-4": {
            "Jramke\\FluidPrimitives\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/fluid_primitives": "*"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "fluid_primitives"
        }
    }
}
